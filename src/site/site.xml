<project xmlns="http://maven.apache.org/DECORATION/1.6.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.6.0 http://maven.apache.org/xsd/decoration-1.6.0.xsd">

    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.1</version>
    </skin>

    <custom>
        <fluidoSkin>
            <topBarEnabled>true</topBarEnabled>
            <topBarIcon>
                <name>NetBeans-SpringBoot</name>
                <alt></alt>
                <src>/images/nb-boot-logo.png</src>
                <href>/index.html</href>
            </topBarIcon>
            <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
            <gitHub>
                <projectId>keevosh/nb-springboot-configuration-support</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>black</ribbonColor>
            </gitHub>
            <twitter>
                <user>mehieltwit</user>
                <showUser>true</showUser>
                <showFollowers>false</showFollowers>
            </twitter>
            <copyrightClass>pull-right</copyrightClass>
        </fluidoSkin>
    </custom>

    <bannerLeft>
        <name>${project.name}</name>
    </bannerLeft>

    <publishDate position="left" />
    <version position="left" />

    <body>
        <head>
            <link rel="stylesheet" type="text/css" href="css/kvsh-maven-theme.css" />
        </head>
        <menu name="Overview">
            <item name="Introduction" href="./index.html"/>
            <item name="Download" href="./overview/download.html"/>
            <item name="Build from source" href="./overview/build.html"/>
        </menu>
        <menu name="Blog">
            <item name="NetBeans+Boot" href="./blog/nb-boot.html"/>
            <item name="Thoughts for v2.0" href="./blog/next.html"/>
        </menu>
        <menu ref="parent" />
        <menu ref="modules" />
        <menu ref="reports" />
    </body>
</project>
